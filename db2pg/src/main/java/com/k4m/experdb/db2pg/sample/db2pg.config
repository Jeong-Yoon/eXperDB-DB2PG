## Whether to perform data extraction of the source DBMS
SRC_EXPORT=FALSE
## Target Database (PostgreSQL) Delete PK, FK, INDEX, Whether to generate script
TAR_CONSTRAINT_EXTRACT=FALSE
## Whether to perform schema extraction of the source DBMS 
SRC_DDL_EXPORT=FALSE

## Source DBMS Information
SRC_HOST=
SRC_USER=
SRC_PASSWORD=
SRC_DATABASE=
SRC_SCHEMA=
SRC_DB_TYPE=ORA
SRC_PORT=1521
SRC_DB_CHARSET=UTF8

## The size of the buffer used by the LOB(each buffers base unit is MiB)
SRC_LOB_FETCH_SIZE=1024
## Number of tables extracted simultaneously to the source DBMS
SRC_TABLE_SELECT_PARALLEL=1
## When the data extracted to the source DBMS is changed to Copy syntax, the number of rows to be executed in one copy syntax
SRC_TABLE_COPY_SEGMENT_SIZE=3000
## Whether main output is displayed on stdout
VERBOSE=TRUE
## Available when all where in the source table extraction query have the same condition
#SRC_WHERE=
## Source Database Catalog table type of view and table
TABLE_ONLY=TRUE
## Set whether Truncate Command should be executed before executing Copy syntax
TRUNCATE=FALSE
## This is an option to specify the table list to be extracted. You must enter a separator between the table names.
#SRC_ALLOW_TABLES=
## This option specifies a table list to be excluded from the extraction. You must enter a separator between the table names.
#SRC_EXCLUDE_TABLES=
## The rows limit to extract to each table.
#SRC_ROWNUM=

## Target DBMS Information
TAR_HOST=
TAR_USER=
TAR_PASSWORD=
TAR_DATABASE=
TAR_SCHEMA=
TAR_DB_TYPE=POG
TAR_PORT=
TAR_DB_CHARSET=UTF8
TAR_CONN_COUNT=1

## Target DBMS copy command option
TAR_COPY_OPTIONS=
## Sam File, Specify the path where DDL is generated
OUTPUT_DIRECTORY=./
## source DBMS case sensitivity(capital, small, original)
CLASSIFY_STRING=original
## You must enter the full path to the queries. xml file that contains the query information to perform the custom select query
#SELECT_QUERIES_FILE=

## each buffers base unit is MiB
BUFFER_SIZE=10
## Number of rows to be requested per communication when extracting through JDBC to the source DBMS
STATEMENT_FETCH_SIZE=3000
## This option specifies the LEVEL for logging.(OFF, FATAL, ERROR, WARN, INFO, DEBUG, TRACE, ALL)
LOG_LEVEL=INFO
## Used when the source DBMS is ASCII encoding.
SRC_IS_ASCII=FALSE

## Setting whether to perform DB load
DB_WRITER_MODE=FALSE
## Set whether to save DB data to a file
FILE_WRITER_MODE=TRUE
## Target table error limit row count
TAR_TABLE_ERR_CNT_EXIT=10
## Target table PK, FK, INDEX deletion and creation setting
TAR_DROP_CREATE_CONSTRAINT=TRUE
