			"^(?i)NUMBER$",
			"^(?i)NUMBER\\s*\\(?[0-9]*\\)?$",
			"^(?i)NUMBER\\s*\\(?[0-9]*,[0]*\\)?$",
			"^(?i)NUMBER\\s*\\(?[0-9]*,[0-9]*\\)?$"
		]
	},
