{
	"comment": {
		"postgres": "#",
		"mysql": [
			"--"
		]
	},
	"string": {
		"postgres": "'",
		"mysql": [
			"\""
		]
	},
	"classify_string": {
		"postgres": "\"",
		"mysql": [
			"`"
		]
	},
	"integer_case_1": {
		"postgres": "SMALLINT",
		"mysql": [
			"^(?i)TINYINT\\s*\\(?[0-9]*\\)\\s*UNSIGNED?$",
			"^(?i)SMALLINT$\\(?[0-9]*\\)?$",
            "^(?i)TINYINT\\s*\\(?[0-9]*\\)?$"
		],
		"mss": [
            "^(?i)TINYINT$"
		]
	},
	"integer_case_2": {
		"postgres": "INTEGER",
		"mysql": [
			"^(?i)MEDIUMINT\\s*\\(?[0-9]*\\)?$",
			"^(?i)SMALLINT\\s*\\(?[0-9]*\\)\\s*UNSIGNED?$",
			"^(?i)MEDIUMINT\\s*\\(?[0-9]*\\)\\s*UNSIGNED?$",
            "^(?i)INT\\s*\\(?[0-9]*\\)?$",
            "^(?i)INTEGER\\s*\\(?[0-9]*\\)?$"
		],
		"mss": [
            "^(?i)INT\\s*\\(?[0-9]*\\)?$",
            "^(?i)INTEGER\\s*\\(?[0-9]*\\)?$"
		],
		 "ora": [
	       "^(?i)INTEGER$",
	       "^(?i)INT$"
	    ]		
	},
	"integer_case_3": {
		"postgres": "BIGINT",
		"mysql": [
			"^(?i)INT\\s*\\(?[0-9]*\\)\\s*UNSIGNED?$",
            "^(?i)INTEGER\\s*\\(?[0-9]*\\)\\s*UNSIGNED?$",
            "^(?i)BIGINT\\s*\\(?[0-9]*\\)?$"
		]
	},
	"integer_case_4": {
